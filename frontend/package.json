{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/node": "^16.18.7",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "build": "react-scripts build",
        "lint:check": "eslint ./src/**/*.{tsx,ts}",
        "lint:fix": "eslint --fix ./src/**/*.{tsx,ts}",
        "fmt:check": "prettier --check './src/**/*.{tsx,ts}' --config ./.prettierrc.json",
        "fmt:fix": "prettier --write './src/**/*.{tsx,ts}' --config ./.prettierrc.json",
        "start": "react-scripts start",
        "test:notestpass": "react-scripts test --passWithNoTests --coverage",
        "test": "react-scripts test --coverage",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^27.5.2",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "eslint": "^8.29.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-functional": "^4.4.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.6.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.31.11",
        "jest": "^29.3.1",
        "prettier": "^2.8.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "*"
    }
}
